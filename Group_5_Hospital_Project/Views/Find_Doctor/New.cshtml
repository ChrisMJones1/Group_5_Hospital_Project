@model List<Group_5_Hospital_Project.Models.Find_Doctor>

@{
    ViewBag.Title = "New";
}

<h2>New Doctor</h2>

<div class="navbar">
    <a class="navlink" href="/Find_Doctor/List">Back to List</a>
</div>
<form method="POST" action="/Find_Doctor/Add">

    <div class="inpfield"><span>doctor_name:</span><input id="doctor_name" type="text" name="doctor_name" /></div>
    <div class="inpfield"><span>doctor_email:</span><input id="doctor_email" type="text" name="doctor_email" /></div>
    <div class="inpfield"><span>doctor_phone:</span><input id="doctor_phone" type="text" name="doctor_phone" /></div>
    <div class="inpfield"><span>doctor_specialization:</span><input id="doctor_specialization" type="text" name="doctor_specialization" /></div>

    <div class="inpfield">
        <input type="submit" value="Add Doctor">
    </div>
</form>

